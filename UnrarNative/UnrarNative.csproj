<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.1</Version>
    <Authors>Muhammad Babayi</Authors>
    <Company>GihanSoft</Company>
    <Description>C# package for use native UnRAR.dll.
with all of methods, structs and helper enums.</Description>
    <PackageProjectUrl>https://github.com/chiefmb/UnrarNative</PackageProjectUrl>
    <PackageTags>rar, compress, unrar</PackageTags>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="UnRAR.dll" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="UnRAR.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
